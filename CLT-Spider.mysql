################################################################################
#     file: CLT-Spider-Revised.mysql
#   author: Ed Thomas
# modified: 12-October-2017
#  purpose: Preliminary/test-bed design of the mysql tables needed to store and
#           maintain the CLT Knowledge Base (CLT_KB) system. Running this script
#           will erase any existing data stored in the CLT_KB. Thus, run this
#           script only when initializing the database, or after a significant
#           re-design of the knowledge base.
#
#
#    Note: The status fields in the InformationLink
#          table indicate the status or degree of completeness of that entry in
#          the database. The potential values and meanings of the status variable
#          are:
#                        0 :: Explored, no Keywords
#                       10 :: Explored, has one or more keywords
#                       20 :: Unexplored, parent has keywords
#                 	30 :: Unexplored
#                       35 :: Profile incomplete (Organization and Contact tables only)
#                       40 :: Access disallowed in robots.txt
#                       50 :: Error condition/ web timeout
#
#   The InformationLinkCategory table lists popular/informative
#   categories that can be associated with data. This is not a fixed list and should be
#   expanded or shortened to suit the needs of the database and its users.
#
################################################################################

DROP DATABASE if exists CLT_KB;

CREATE DATABASE CLT_KB;

USE CLT_KB;

create table InformationLink(
       linkID			int,
       linkURL          	varchar(512),
       linkTitle        	varchar(256),
       linkSynopsis		varchar(1024),
       referrerID		int,
       downloadPath		varchar(1024),
       discovered		date,
       lastExplored		date,
       keywordCount		int,
       status                   int,
       level			int,
       category			int,
       
       primary key (linkID),
       index (linkID),
       index (linkURL)
);


create table TrackLinkID (
       linkID		int
);

insert into TrackLinkID values(100);



create table KeywordLinkage(
       linkID		int NOT NULL,
       keywordID	int NOT NULL,

       primary key (linkID, keywordID)
);


CREATE TABLE categories (
       category_id	 int(11) NOT NULL AUTO_INCREMENT,
       category_name	  varchar(50) NOT NULL,
       
       PRIMARY KEY (category_id),
       UNIQUE KEY category_name (category_name)
);



